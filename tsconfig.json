{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "lib": ["dom", "es2017", "es2018.promise", "ES2021.String"],
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./resources/js/*"]
    }
  },
  "exclude": [
    "node_modules"
  ],
}
